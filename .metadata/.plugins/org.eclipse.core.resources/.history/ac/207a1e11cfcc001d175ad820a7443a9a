package Pessoas;

public class Pessoas {

	public static void main(String[] args) {
		
		Fisica fis = new Fisica();
		
		fis.nome = "João";
		fis.cpf	= "1111111";
		fis.identidade = "1500445";
		fis.situacaoPessoa = "A";
		
		Juridica jur = new Juridica();
		
		jur.nome = "CASAS DA SILVA";
		jur.cnpj = "182371092";
		jur.inscEstadual = "3452352";
		jur.situacaoPessoa = "I";
		
		System.out.println("Dados da pessoa fisíca");
		fis.toString()
		System.out.println("Dados da pessoa juridica");
	}
}
