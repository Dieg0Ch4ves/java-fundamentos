package fundamentos;

public class Inferencia {
	public static void main(String[] args) {
		double teste = 12;
		teste = 13 ;
		System.out.println(teste);
		//teste = "13";
		System.out.println(teste);
		
		var teste2 = "13";
		System.out.println(teste2);
		teste2 = "14";
		System.out.println(teste2);
		//teste2 = 11;
		System.out.println(teste2);
		
		var teste3 ;
		teste3 = "12";
		//teste3 = 12;
		System.out.println(teste3);
		
	}
}
